<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd"
    logicalFilePath="changelog-4.7.1-add-lot-and-labels.xml">

    <changeSet author="author" id="changelog-4.7.2-add-tc-wenglor.xml">      	
        <createTable tableName="tc_wenglor">
            <column name="id" type="INT" autoIncrement="true">
                <constraints primaryKey="true" />
            </column>
            <column name="deviceid" type="INT">
                <constraints nullable="false" />
            </column>         
            <column name="created" type="TIMESTAMP" defaultValueComputed="CURRENT_TIMESTAMP">
                <constraints nullable="false" />
            </column>             
            <column name="protocol" type="VARCHAR(128)" />            
            <column name="message" type="VARCHAR(128)" />            
            <column name="description" type="VARCHAR(1000)" />     
            <column name="p1" type="VARCHAR(32)"/>                                       
            <column name="p2" type="VARCHAR(32)"/>
            <column name="p3" type="VARCHAR(32)"/>
            <column name="p4" type="VARCHAR(32)"/>
            <column name="p5" type="VARCHAR(32)"/>
            <column name="p6" type="VARCHAR(32)"/>
            <column name="p7" type="VARCHAR(32)"/>
            <column name="p8" type="VARCHAR(32)"/>                                    
        </createTable>      
    </changeSet>

</databaseChangeLog>
